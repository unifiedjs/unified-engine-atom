{
  "name": "unified-engine-atom",
  "version": "1.1.0",
  "description": "Create Atom Linters for unified processors",
  "license": "MIT",
  "keywords": [
    "atom",
    "linter",
    "unified",
    "processor"
  ],
  "files": [
    "index.js"
  ],
  "repository": "https://github.com/wooorm/unified-engine-atom",
  "bugs": "https://github.com/wooorm/unified-engine-atom/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "dependencies": {
    "atom-tap-test-runner": "^1.0.0",
    "find-root": "^1.0.0",
    "to-vfile": "^1.0.0",
    "unified-engine": "^1.3.0",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "remark": "^5.0.0",
    "remark-cli": "^1.0.0",
    "remark-comment-config": "^4.0.0",
    "remark-github": "^5.0.0",
    "remark-lint": "^4.0.0",
    "remark-validate-links": "^5.0.0",
    "tape": "^4.6.0",
    "xo": "^0.16.0"
  },
  "scripts": {
    "build": "remark *.md --quiet --frail",
    "lint": "xo",
    "test-api": "atom --test test",
    "test": "npm run build && npm run lint && npm run test-api"
  },
  "atomTestRunner": "atom-tap-test-runner",
  "xo": {
    "space": true
  },
  "remarkConfig": {
    "output": true,
    "plugins": [
      "comment-config",
      "github",
      "lint",
      "validate-links"
    ],
    "settings": {
      "bullet": "*"
    }
  }
}
